@using MediPlus.DAL.Models;
@model Appointment;

<form method="post">
    <div class="form-group">
        <label asp-for="DoctorId"></label>
        <input class="form-control" asp-for="DoctorId">
    </div>
    <div class="form-group">
        <label asp-for="PatientId"></label>
        <input class="form-control" asp-for="PatientId">
    </div>
    <div class="form-group">
        <label asp-for="AppointmentDate"></label>
        <input class="form-control" asp-for="AppointmentDate">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>